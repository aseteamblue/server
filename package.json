{
  "name": "server",
  "version": "1.0.0",
  "description": "server",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/index.js",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js | bunyan",
    "devWindows": "SET NODE_ENV=development&& babel-node src/index.js | bunyan",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aseteamblue/server.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aseteamblue/server/issues"
  },
  "homepage": "https://github.com/aseteamblue/server#readme",
  "dependencies": {
    "@koa/cors": "^2.2.2",
    "bcryptjs": "^2.4.3",
    "bunyan": "^1.8.12",
    "dotenv": "^6.1.0",
    "koa": "^2.5.3",
    "koa-bodyparser": "^4.2.1",
    "koa-mongo": "^1.4.1",
    "koa-passport": "^4.1.1",
    "koa-router": "^7.4.0",
    "koa-session": "^5.9.0",
    "mongoose": "^5.3.2",
    "mqtt": "^2.18.8",
    "passport-local": "^1.0.0",
    "swagger-jsdoc": "^3.2.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.24.1",
    "babel-polyfill": "^6.8.0",
    "eslint": "^5.6.1",
    "eslint-config-kasa": "^0.1.3",
    "nodemon": "^1.18.4"
  }
}
